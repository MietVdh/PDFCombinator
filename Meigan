CREATE TABLE `PDFCombinator` (
	`id` char(10) COLLATE utf8_unicode_ci NOT NULL,
    `FirstName` char(40) COLLATE utf8_unicode_ci NOT NULL,
    `LastName` char(40) COLLATE utf8_unicode_ci NOT NULL,
    `Username` char(25) COLLATE utf8_unicode_ci NOT NULL,
    `Email` char(40) COLLATE utf8_unicode_ci NOT NULL,
    `Password` char(30) COLLATE utf8_unicode_ci NOT NULL,
    PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci;
